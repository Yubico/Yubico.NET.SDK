{
  "metadata": [
    {
      "src": [
        {
          "files": ["Yubico.Core.csproj"],
          "src": "./Yubico.Core/src/",
          "exclude": ["**/bin/**", "**/obj/**"]
        }
      ],
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
      "memberLayout": "separatePages",
      "filter": "./docs/filterConfig.yml",
      "dest": "./docs/core-api",
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    },
    {
      "src": [
        {
          "files": ["Yubico.YubiKey.csproj"],
          "src": "./Yubico.YubiKey/src/",
          "exclude": ["**/bin/**", "**/obj/**"]
        }
      ],
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
      "memberLayout": "separatePages",
      "filter": "./docs/filterConfig.yml",
      "dest": "./docs/yubikey-api",
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "yubikey-api/**.yml",
          "yubikey-api/index.md",
          "core-api/**.yml",
          "core-api/index.md",
          "users-manual/**.md",
          "users-manual/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "src": "./docs/"
      }
    ],
    "resource": [
      {
        "files": ["images/**"],
        "src": "./docs/"
      }
    ],
    "overwrite": [
      {
        "files": ["namespaces/**.md"],
        "exclude": ["obj/**", "_site/**"],
        "src": "./docs/"
      }
    ],
    "dest": "./docs/_site/",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appFaviconPath": "images/favicon.ico"
    },
    "fileMetadataFiles": [],
    "template": ["default", "./docs/memberpage", "./docs/yubikeysdk-template"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
